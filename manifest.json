{
    "name": "SSO privacy extension",
    "description": "A privacy extension that shows data requested with SSO logins.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "src/js/background.js"
    },
    "action": {
      "default_popup": "src/popup.html"
    },
    "permissions": [
      "webRequest"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
        {
          "matches": [
            "http://*/*",
            "https://*/*"
          ],
          "js": ["src/js/contentscript.js"]
        }
    ]
  }
  